{
  "autoload": {
    "psr-4": {
      "Dna\\Payment\\": "Dna/Payment"
    },
    "files": [
      "Dna/Payment/registration.php"
    ]
  },
  "config": {
    "allow-plugins": {
      "magento/composer-dependency-version-audit-plugin": true,
      "magento/magento-composer-installer": true
    }
  },
  "description": "DNA payments gateway for magento 2",
  "license": [
    "OSL-3.0",
    "AFL-3.0"
  ],
  "minimum-stability": "dev",
  "name": "dna/magento-2",
  "prefer-stable": true,
  "repositories": {
    "magento": {
      "type": "composer",
      "url": "https://repo.magento.com"
    }
  },
  "require": {
    "php": ">=7.0",
    "magento/module-sales": "*",
    "magento/module-checkout": "*",
    "magento/module-payment": "*",
    "magento/framework": "*",
    "dna/dnapayments-sdk-php": "2.1.2",
    "magento/module-vault": "^101.0.0"
  },
  "require-dev": {
    "magento/magento-composer-installer": "0.4.0"
  },
  "type": "magento2-module",
  "version": "1.6.0"
}
