{
  "name": "dna/magento-2",
  "description": "DNA payments gateway for magento 2",
  "type": "magento2-module",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=5.5.0",
    "magento/module-sales": "*",
    "magento/module-checkout": "*",
    "magento/module-payment": "*",
    "magento/framework": "*",
    "dna/dnapayments-sdk-php": "1.0.0"
  },
  "require-dev": {
    "magento/magento-composer-installer": "^0.1.13"
  },
  "license": [
    "OSL-3.0",
    "AFL-3.0"
  ],
  "autoload": {
    "psr-4": {
      "Dna\\Payment\\": "Dna/Payment"
    },
    "files": [
      "Dna/Payment/registration.php"
    ]
  },
  "repositories": {
    "magento": {
      "type": "composer",
      "url": "https://repo.magento.com"
    }
  }
}
