<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
    <route url="/V1/dna-payment/confirm" method="POST">
        <service class="Dna\Payment\Api\OrderManagementInterface" method="confirmOrder"/>
        <resources>
            <resource ref="anonymous" />
        </resources>
    </route>
    <route url="/V1/dna-payment/close" method="POST">
        <service class="Dna\Payment\Api\OrderManagementInterface" method="closeOrder"/>
        <resources>
            <resource ref="anonymous" />
        </resources>
    </route>
</routes>
